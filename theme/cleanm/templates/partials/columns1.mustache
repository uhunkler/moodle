<header role="banner"
        class="navbar navbar-fixed-top {{{html_navbarclass}}} moodle-has-zindex">
    <nav role="navigation" class="navbar-inner">
        <div class="container-fluid">
            <a class="brand" href="{{{wwwroot}}}">{{{shortname}}}</a>
            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </a>
            {{{user_menu}}}
            <div class="nav-collapse collapse">
                {{{custom_menu}}}
                <ul class="nav pull-right">
                    <li>{{{page_heading_menu}}}</li>
                </ul>
            </div>
        </div>
    </nav>
</header>

<div id="page" class="container-fluid">
    <header id="page-header" class="clearfix">
        {{{html_heading}}}
        <div id="page-navbar" class="clearfix">
            <nav class="breadcrumb-nav">{{{navbar}}}</nav>
            <div class="breadcrumb-button">{{{page_heading_button}}}</div>
        </div>
        <div id="course-header">
            {{{course_header}}}
        </div>
    </header>

    <div id="page-content" class="row-fluid">
        <section id="region-main" class="span12">
            {{{course_content_header}}}
            <span>Mustache template</span>
            {{{main_content}}}
            {{{course_content_footer}}}
        </section>
    </div>

    <footer id="page-footer">
        <div id="course-footer">{{{course_footer}}}</div>
        <p class="helplink">{{{page_doc_link}}}</p>
        {{{html_footnote}}}
        {{{login_info}}}
        {{{home_link}}}
        {{{standard_footer_html}}}
    </footer>
</div>
